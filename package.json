{
  "name": "generator-paragons",
  "version": "3.1.0",
  "description": "Yeoman generator for scaffolding paragons",
  "homepage": "http://joseki-tech.com/",
  "author": {
    "name": "Stephen Drahus",
    "email": "stephen.drahus@joseki-tech.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joseki-tech/generator-paragons.git"
  },
  "bin": {
    "generator-paragons": "./cli.js"
  },
  "main": "generators/index.js",
  "keywords": [
    "spa",
    "universal",
    "isomorphic",
    "react",
    "router",
    "redux",
    "code splitting",
    "ssr",
    "server side render",
    "sass",
    "seo",
    "search engine optimization"
  ],
  "devDependencies": {
    "eslint": "5.9.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-config-xo": "0.25.1",
    "eslint-plugin-prettier": "3.0.0",
    "husky": "1.2.0",
    "jest": "26.0.1",
    "lint-staged": "8.1.0",
    "prettier": "1.15.3",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "1.9.1",
    "yo": "3.1.1"
  },
  "dependencies": {
    "chalk": "2.4.1",
    "lodash": "4.17.15",
    "yeoman-generator": "3.1.1",
    "yosay": "2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublishOnly": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "license": "Apache-2.0"
}
